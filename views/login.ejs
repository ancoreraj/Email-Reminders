<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-2" style="font-size: 50px;">Raven</h1>

      <% include ./partials/messages %>
        <form action="/contact" method="POST">
          <div class="form-group">
            <input type="text" id="email" name="email" class="form-control" placeholder="Enter Your Name" />
          </div>
          <div class="form-group">
            <input id="password" name="password" class="form-control" placeholder="Enter Your Contact No" type="text"
              maxlength="10"/>
          </div>
          <button type="submit" class="btn btn-primary btn-block">Submit</button>
        </form>
    </div>
  </div>
</div>